pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'abeerab/backimage' }
      }
      steps {
        sh 'node --version'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'abeerab/firstimage' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}
